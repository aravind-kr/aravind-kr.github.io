{
  "name": "github-page",
  "version": "1.0.0",
  "description": "personal portfolio page",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "deploy": "next build && next export",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "test-win": "SET NODE_ENV=test&& jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravind-kr/aravind.github.io.git"
  },
  "author": "Aravind",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aravind-kr/aravind.github.io/issues"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.2",
    "@material-ui/icons": "^3.0.2",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.0",
    "bootstrap": "^4.2.1",
    "dotenv": "^6.2.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-dom": "^16.7.0",
    "react-icons": "^3.2.2",
    "react-jss": "^8.6.1",
    "react-particles-js": "^2.4.2",
    "react-typist": "^2.0.5",
    "react-window-size": "^1.2.2",
    "reactstrap": "^7.0.2",
    "styled-components": "^4.1.3"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
